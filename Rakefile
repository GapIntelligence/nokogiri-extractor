# -*- ruby -*-

require "rubygems"
require "hoe"

Hoe.plugin :minitest

Hoe.spec "nokogiri-extractor" do
   developer("Ethan Soutar-Rau", "ethan@gapintelligence.com")

   self.group_name = "gap intelligence"
   self.extra_deps << ['nokogiri', '~> 1.6']
   self.extra_deps << ['andand',   '~> 1.3']

   license "MIT"
end

